class Solution {
public:
    // Helper function to find GCD (using Euclidean Algorithm)
int gcd(int a, int b) {
    while (b) {
        a = a % b;
        swap(a, b);
    }
    return a;
}
    string gcdOfStrings(string str1, string str2) {
        if(str1+str2 != str2+str1){
            return "";
        }
        int l1 = str1.length();
        int l2 = str2.length();
        int l = gcd(l1,l2);
        if(l1<l2){
            return str1.substr(0,l);
        }else{
            return str2.substr(0,l);
        }

    }
};
